{
  "manifest_version": 3,
  "name": "Todo Google Sheets",
  "version": "1.0.0",
  "description": "Todo list that syncs with Google Sheets",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["identity"],
  "oauth2": {
    "client_id": "612661509188-qa4792tn8qpuo9amntk9p7ddtuamgrb8.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "host_permissions": ["https://*.googleapis.com/*"],
  "key": ""
}
